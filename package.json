{
	"name": "next-demo-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"lint": "next lint --fix",
		"prepare": "husky install",
		"start": "next start"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{css,json,md,yml,yaml}": [
			"prettier --write"
		]
	},
	"prettier": {
		"importOrder": [
			"^@core/(.*)$",
			"^@server/(.*)$",
			"^@ui/(.*)$",
			"^[./]"
		],
		"importOrderSeparation": true,
		"importOrderSortSpecifiers": true
	},
	"dependencies": {
		"@apihero/browser": "1.3.16",
		"@giscus/react": "2.4.0",
		"@happykit/analytics": "1.0.1",
		"@happykit/flags": "2.0.7",
		"@hookform/error-message": "2.0.1",
		"@hookform/resolvers": "3.10.0",
		"@tailwindcss/typography": "0.5.16",
		"daisyui": "4.12.23",
		"next": "14.2.23",
		"next-swagger-doc": "0.4.1",
		"next-validations": "0.4.2",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-hook-form": "7.54.2",
		"react-query": "3.39.3",
		"sharp": "0.33.5",
		"swagger-ui-react": "5.18.2",
		"yup": "1.6.1"
	},
	"devDependencies": {
		"@codechecks/build-size-watcher": "0.1.0",
		"@codechecks/client": "0.1.12",
		"@hookform/devtools": "4.3.3",
		"@next/eslint-plugin-next": "14.2.23",
		"@trivago/prettier-plugin-sort-imports": "4.3.0",
		"@types/node": "20.17.12",
		"@types/react": "18.3.18",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"autoprefixer": "10.4.20",
		"eslint": "9.19.0",
		"eslint-config-next": "14.2.23",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-productsway": "1.3.4",
		"husky": "8.0.3",
		"lint-staged": "15.3.0",
		"postcss": "8.5.0",
		"prettier": "3.4.2",
		"tailwindcss": "3.4.17",
		"typescript": "5.7.3"
	},
	"packageManager": "yarn@4.6.0",
	"apihero": {
		"workerDirectory": "public"
	}
}
